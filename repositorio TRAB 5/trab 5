1- def imprime_vogais(frase):
    vogais = "aeiouAEIOU"
    vogais_na_frase = [letra for letra in frase if letra in vogais]
    for vogal in vogais_na_frase:
        print(vogal, end=' ')

# Exemplo de uso
frase = "Implementando um algoritmo em Python"
imprime_vogais(frase)

2-def bubbleSort(alist):
    for passnum in range(len(alist)-1,0,-1):
        for i in range(passnum):
            if alist[i]>alist[i+1]:
                temp = alist[i]
                alist[i] = alist[i+1]
                alist[i+1] = temp

alist = [54,26,93,17,77,31,44,55,20]
bubbleSort(alist)
print(alist)
